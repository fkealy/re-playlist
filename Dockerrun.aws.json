{
    "AWSEBDockerrunVersion": "3",
    "containerDefinitions": [
      {
        "name": "replaylist-container",
        "image": "588324452158.dkr.ecr.eu-west-2.amazonaws.com/replaylist:Latest",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 3000,
            "containerPort": 3000
          }
        ],
        "environment": [
          {
            "name": "PORT",
            "value": "3000"
          },
          {
            "name": "APP_NAME",
            "value": "RE:PLAYLIST"
          }
        ]
      }
    ]
  }